
@model DepartmentVM

@{
    ViewData["Title"] = "Details";
}

<div class="block-header">
   <h2>Details For @Model.Name  Department</h2>
</div>



    <label asp-for="Name">Department Name</label>
    <input type="text" class="form-control" asp-for="Name" readonly />
    <br />

    <label asp-for="Code">Department Code</label>
    <input type="text" class="form-control" asp-for="Code" readonly />
    <br />


    <center>
    <a asp-controller="Department" asp-action="Index" class="btn btn-primary">Return</a>
    <a asp-controller="Department" asp-action="Update" asp-route-id="@Model.Id" class="btn btn-warning">Update</a>
    <a asp-controller="Department" asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-primary">Delete</a>
    </center>